{
    "name": "harumaro/microsoft-luis-sdk",
    "description": "SDK for the Microsoft Luis API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Raffaele Procentese",
            "email": "deadpikasso@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.3",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": { "LUIS\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "LUIS\\Tests\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
